apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: {{ limit_all_trafico_nombre }}
spec:
  podSelector:
    matchLabels:
      {{ limit_all_trafico_pod_matchlabels | to_nice_yaml(indent=2) | trim | indent(6) }}
  ingress:
  - from:
      - podSelector:
          matchLabels:
            {{ limit_all_trafico_pod_matchlabels | to_nice_yaml(indent=2) | indent(12) }}